PROJECT_NAME = "@PROJECT_NAME@"
PROJECT_NUMBER = "@PROJECT_VERSION@"
PROJECT_BRIEF = "@PROJECT_DESCRIPTION@"

INPUT = "@PROJECT_SOURCE_DIR@/src" "@PROJECT_SOURCE_DIR@/docs/pages"
EXTRACT_ALL = NO
ENABLED_SECTIONS =
EXAMPLE_PATH = "@PROJECT_SOURCE_DIR@"
EXTRACT_PRIVATE = NO
HIDE_UNDOC_MEMBERS = NO
EXCLUDE_PATTERNS = "@PROJECT_SOURCE_DIR@/conanfile.py" "@PROJECT_SOURCE_DIR@/*.test.*" "@PROJECT_BINARY_DIR@/*"
RECURSIVE = YES
OUTPUT_DIRECTORY = "@DOXYGEN_OUTPUT_DIRECTORY@"
FULL_PATH_NAMES = YES
STRIP_FROM_PATH = "@PROJECT_SOURCE_DIR@" "@PROJECT_SOURCE_DIR@/src" "@PROJECT_SOURCE_DIR@/docs/pages"

GENERATE_TREEVIEW = YES
HTML_COLORSTYLE = LIGHT

SHOW_FILES = NO
SHOW_INCLUDE_FILES = NO

HAVE_DOT = YES
DOT_IMAGE_FORMAT = svg
INTERACTIVE_SVG = YES
FULL_SIDEBAR = YES

WARN_IF_UNDOCUMENTED = NO
