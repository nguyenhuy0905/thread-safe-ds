PROJECT_NAME = "@PROJECT_NAME@"
PROJECT_NUMBER = "@PROJECT_VERSION@"

INPUT = "@PROJECT_SOURCE_DIR@" "@PROJECT_SOURCE_DIR@/src"
DISTRIBUTE_GROUP_DOC = YES
EXTRACT_ALL = NO
ENABLED_SECTIONS =
EXTRACT_PRIVATE = NO
HIDE_UNDOC_MEMBERS = YES
EXCLUDE_PATTERNS = "@PROJECT_SOURCE_DIR@/conanfile.py" "@PROJECT_SOURCE_DIR@/docs/*" "@PROJECT_SOURCE_DIR@/*.test.*" "@PROJECT_BINARY_DIR@" "@PROJECT_SOURCE_DIR@/build"
HTML_COLORSTYLE = LIGHT
RECURSIVE = YES
OUTPUT_DIRECTORY = "@DOXYGEN_OUTPUT_DIRECTORY@"
USE_MDFILE_AS_MAINPAGE = "@PROJECT_SOURCE_DIR@/README.md"
FULL_PATH_NAMES = YES
STRIP_FROM_PATH = "@PROJECT_SOURCE_DIR@" "@PROJECT_SOURCE_DIR@/src"
